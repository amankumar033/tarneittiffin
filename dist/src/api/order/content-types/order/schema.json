{
    "kind": "collectionType",
    "collectionName": "orders",
    "info": {
        "singularName": "order",
        "pluralName": "orders",
        "displayName": "order"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "address": {
            "type": "string"
        },
        "total_cost": {
            "type": "decimal"
        },
        "additonal_equipments": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::additonal-equipment.additonal-equipment",
            "inversedBy": "orders"
        },
        "full_name": {
            "type": "string"
        },
        "email": {
            "type": "email"
        },
        "phone_number": {
            "type": "biginteger"
        },
        "food_item": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::food-item.food-item"
        },
        "crockery": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::crockery.crockery"
        },
        "date_time": {
            "type": "datetime"
        },
        "order_status": {
            "type": "enumeration",
            "enum": [
                "SCHEDULED",
                "DELIVERED",
                "CANCELLED"
            ]
        }
    }
}
